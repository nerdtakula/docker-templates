<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/nerdtakula/docker-templates/master/bookshelf/bookshelf.xml</TemplateURL>
  <Beta>True</Beta>
  <Category>MediaServer:Book</Category>
  <Name>nerdtakula-bookshelf</Name>
  <Description>Service to display</Description>
  <Overview></Overview>
  <Support>---</Support>
  <Registry>https://registry.hub.docker.com/u/nerdtakula/bookshelf/</Registry>
  <GitHub>https://github.com/nerdtakula/bookshelf</GitHub>
  <Repository>nerdtakula/bookshelf</Repository>
  <Project>https://github.com/nerdtakula/bookshelf</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>UMASK</Name>
      <Value>000</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/config</HostDir>
      <ContainerDir>/opt/bookshelf/config</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/opt/bookshelf/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user</HostDir>
      <ContainerDir>/opt/bookshelf/static/pdfs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <Icon>https://raw.githubusercontent.com/nerdtakula/docker-templates/master/bookshelf/bookshelf-icon.png</Icon>
  <DonateText>---</DonateText>
  <DonateLink>---</DonateLink>
  <DonateImg>---</DonateImg>
  <ExtraParams></ExtraParams>
</Containers>
